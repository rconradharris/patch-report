class Repo(object):
    def __init__(self, url, ssh_url=None, name=None):
        self.url = url
        self.ssh_url = ssh_url
        self._name = name
   
    @property
    def name(self):
        if self._name:
            return self._name
        
        # Determine from url
        return 'thing'


